#include<stdio.h>

int main()
{
	printf("     **     \n");
	printf("     **     \n");
	printf("************\n");
	printf("************\n");
	printf("    * *     \n");
	printf("    * *     \n");
	return 0;
} 




OR
#include<stdio.h>

int main()
{
	printf("     **     \n     **     \n************\n************\n    * *     \n    * *     \n");
	return 0;
}
