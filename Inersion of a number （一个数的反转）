#define _CRT_SECURE_NO_WARNINGS

#include<stdio.h>

int main()
{
	int n = 0;
	int a = -1;
	scanf("%d", &n);
	while (n < 0)
	{
		printf("%d", n % 10);
		n = a * n;
		n /= 10;
	}
	while (n > 0)
	{
		printf("%d", n % 10);
		n /= 10;
	}
	while (n == 0)
	{
		printf("无不无聊？( you are so annoyed haha)");
	}
	return 0;
}
