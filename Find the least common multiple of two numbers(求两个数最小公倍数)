#define _CRT_SECURE_NO_WARNINGS

#include<stdio.h>

int main()
{
	int num1 = 0;
	int num2 = 0;
	int sum  = 0;
	printf("请输入两个数(please enter two numbers)：\n");
	scanf("%d %d", &num1, &num2);
	if (num1 > num2)
	{
		if (num1 % num2 == 0)
		{
			printf("最小公倍数(least common multiple)：%d\n", num1);
		}
		else
		{
			sum = num1 * num2;
			printf("最小公倍数(least common multiple)：%d\n", sum);
		}
	}
	else
	{
		sum = num1 * num2;
		printf("最小公倍数(least commonn multiple)：%d\n", sum);
	}
	return 0;
}
