#define _CRT_SECURE_NO_WARNINGS

#include<stdio.h>
int main()
{
    int arr[10] = { 0 };
    int i = 0;

    printf("请输入十个整数(Please enter ten intergers):");
    for (i = 0; i < 10; i++)
    {
        scanf("%d", &arr[i]);
    }

    int max = arr[0];

    for (i = 1; i < 10; i++)
    {
        if (arr[i] > max)
        {
            max = arr[i];
        }
    }
    printf("%d", max);
    return 0;
}
